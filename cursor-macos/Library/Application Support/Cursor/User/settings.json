{
  "terminal.external.osxExec": "iTerm.app",
  "editor.formatOnType": true,
  "github.gitProtocol": "ssh",
  "editor.fontLigatures": true,
  "editor.inlineSuggest.enabled": true,
  "window.autoDetectColorScheme": true,
  "editor.semanticHighlighting.enabled": true,
  "jupyter.askForKernelRestart": false,
  "jupyter.widgetScriptSources": ["jsdelivr.com", "unpkg.com"],
  "semantic-highlighting.colors": [
    "#529D52",
    "#BE7070",
    "#3D7676",
    "#BE9970",
    "#9D527C"
  ],
  "python.analysis.typeCheckingMode": "off",
  "[python]": {
    "editor.formatOnSave": true,
    "editor.codeActionsOnSave": {
      "source.fixAll": "explicit",
      "source.organizeImports": "explicit"
    },
    "editor.defaultFormatter": "charliermarsh.ruff"
  },
  "editor.fontSize": 26,
  "jupyter.themeMatplotlibPlots": true,
  "debug.console.fontSize": 22,
  "scm.inputFontSize": 24,
  "chat.editor.fontSize": 22,
  "markdown.preview.fontSize": 26,
  "files.insertFinalNewline": true,
  "remote.autoForwardPortsSource": "hybrid",
  "svifpd.matplotlibUseAgg": true,
  "editor.minimap.size": "fill",
  "editor.minimap.showSlider": "always",
  "lldb.library": "/Applications/Xcode.app/Contents/SharedFrameworks/LLDB.framework/Versions/A/LLDB",
  "lldb.launch.expressions": "native",
  "terminal.integrated.fontSize": 26,
  "python.defaultInterpreterPath": "/Users/anatoli/.pyenv/versions/3.11.3/bin/python",
  "diffEditor.maxComputationTime": 0,
  "cursor.general.enableShadowWorkspace": true,
  "cursor.cpp.enablePartialAccepts": true,
  "semantic-highlighting.highlightGlobals": true,
  "editor.minimap.autohide": true,
  "editor.minimap.scale": 3,
  "editor.minimap.enabled": true,
  "terminal.integrated.fontFamily": "MesloLGS NF",
  "cursor.composer.shouldAllowCustomModes": true,
  "notebook.defaultFormatter": "charliermarsh.ruff",
  "editor.defaultFormatter": "charliermarsh.ruff",
  "remote.downloadExtensionsLocally": true,

  "window.titleBarStyle": "custom",
  "vscode_vibrancy.enableAutoTheme": true,
  "vscode_vibrancy.forceFramelessWindow": true,
  "vscode_vibrancy.opacity": 0.0,
  "vscode_vibrancy.preferedDarkTheme": "Default Dark",
  "vscode_vibrancy.preferedLightTheme": "Default Light",
  "workbench.colorTheme": "Default Dark+",
  "workbench.preferredDarkColorTheme": "Default Dark+",
  "workbench.preferredLightColorTheme": "Default Light+",
  "vscode_vibrancy.theme": "Default Light",
  "workbench.colorCustomizations": {
    "[Default Dark]": {
      "terminal.ansiBlack": "#110034",
      "terminal.ansiRed": "#FF4F44",
      "terminal.ansiGreen": "#00C8AB",
      "terminal.ansiYellow": "#FFD44F",
      "terminal.ansiBlue": "#8217FF",
      "terminal.ansiMagenta": "#FFC9D7",
      "terminal.ansiCyan": "#00E6BB",
      "terminal.ansiWhite": "#F3F0DF",
      "terminal.ansiBrightBlack": "#330D81",
      "terminal.ansiBrightRed": "#FF6767",
      "terminal.ansiBrightGreen": "#00E6BB",
      "terminal.ansiBrightYellow": "#FFE680",
      "terminal.ansiBrightBlue": "#9A67FF",
      "terminal.ansiBrightMagenta": "#FFC9D7",
      "terminal.ansiBrightCyan": "#00E6CC",
      "terminal.ansiBrightWhite": "#F3F0DF",
      "terminal.background": "#110034",
      "terminal.foreground": "#F3F0DF",
      "terminal.selectionBackground": "#330D81",
      "terminalCursor.foreground": "#8217FF",
      "terminalCursor.background": "#110034",
      "terminal.findMatchBackground": "#FFD44F"
    },
    "[Default Light+]": {
      "terminal.ansiBlack": "#110034",
      "terminal.ansiRed": "#CC3E34",
      "terminal.ansiGreen": "#009A81",
      "terminal.ansiYellow": "#CD9A1B",
      "terminal.ansiBlue": "#330D81",
      "terminal.ansiMagenta": "#E69AB3",
      "terminal.ansiCyan": "#00B39A",
      "terminal.ansiWhite": "#F3F0DF",
      "terminal.ansiBrightBlack": "#330D81",
      "terminal.ansiBrightRed": "#CC3E34",
      "terminal.ansiBrightGreen": "#00B39A",
      "terminal.ansiBrightYellow": "#E6B334",
      "terminal.ansiBrightBlue": "#6734CD",
      "terminal.ansiBrightMagenta": "#E69AB3",
      "terminal.ansiBrightCyan": "#009A81",
      "terminal.ansiBrightWhite": "#F3F0DF",
      "terminal.background": "#F3F0DF",
      "terminal.foreground": "#110034",
      "terminal.selectionBackground": "#B39AFF",
      "terminalCursor.foreground": "#8217FF",
      "terminalCursor.background": "#F3F0DF",
      "terminal.findMatchBackground": "#FFD44F"
    },
    "terminal.background": "#00000000"
  },
  "terminal.integrated.drawBoldTextInBrightColors": true,
  "redhat.telemetry.enabled": false,
  "[github-actions-workflow]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "remote.SSH.remotePlatform": {
    "Simply-Workstation": "linux"
  },
  "workbench.settings.openDefaultKeybindings": true,
  "vscode-neovim.neovimExecutablePaths.darwin": "/opt/homebrew/bin/nvim",
  "extensions.experimental.affinity": {
    "asvetliakov.vscode-neovim": 1
  },
  "terminal.integrated.gpuAcceleration": "off"
}
